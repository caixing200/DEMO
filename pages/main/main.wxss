.container {
  height: 100%;
  overflow: hidden;
}

.card{ 
  height: 180rpx;
  width: 706rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  margin-top: 20rpx;
  position: relative;
  background-color: #ffffff;
  border-radius: 4rpx;
  box-shadow: 2rpx 2rpx 1rpx #ebebeb;
}

.icon{
  width: 40rpx;
  height: 40rpx;
  margin-left: 24rpx;
  margin-right: 15rpx;
}

.card-text{
  display: flex;
  flex-direction: row;
  font-size: 38rpx;
  justify-content: left;
  height: 186rpx;
  align-items: center;
}

